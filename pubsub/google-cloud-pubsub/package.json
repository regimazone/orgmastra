{
  "name": "@mastra/google-cloud-pubsub",
  "version": "0.1.4",
  "description": "Mastra Google Cloud PubSub integration",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "CHANGELOG.md"
  ],
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.cjs"
      }
    },
    "./package.json": "./package.json"
  },
  "scripts": {
    "build": "tsup --silent --config tsup.config.ts",
    "build:watch": "tsup --watch --silent --config tsup.config.ts",
    "test": "vitest run --bail=1",
    "lint": "eslint ."
  },
  "dependencies": {
    "@google-cloud/pubsub": "^5.1.0",
    "@inngest/realtime": "^0.3.1",
    "@opentelemetry/api": "^1.9.0",
    "inngest": "^3.40.2",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "ai": "^4.3.19",
    "@internal/lint": "workspace:*",
    "@internal/types-builder": "workspace:*",
    "@mastra/core": "workspace:*",
    "@mastra/deployer": "workspace:*",
    "@mastra/libsql": "workspace:*",
    "@types/node": "^20.19.0",
    "typescript": "^5.8.3",
    "eslint": "^9.30.1",
    "vitest": "^3.2.4",
    "tsup": "^8.4.0"
  },
  "peerDependencies": {
    "@mastra/core": ">=0.15.3-0 <0.17.0-0"
  },
  "homepage": "https://mastra.ai",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mastra-ai/mastra.git",
    "directory": "pubsub/google-cloud-pubsub"
  },
  "bugs": {
    "url": "https://github.com/mastra-ai/mastra/issues"
  }
}
