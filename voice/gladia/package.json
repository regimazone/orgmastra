    "eslint": "^9.30.1",
    "pnpm": "10.12.4",
    "tsup": "^8.4.0",
    "typescript": "^5.8.3",
    "vitest": "^3.2.4",
    "@internal/types-builder": "workspace:*",
    "zod": "^3.25.76"
