    "@webcontainer/env": "^1.1.1",
    "commander": "^12.1.0",
    "dotenv": "^16.6.1",
    "execa": "^9.6.0",
    "fs-extra": "^11.3.1",
    "get-port": "^7.1.0",
    "open": "^10.1.2",
