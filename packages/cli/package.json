    "cpy-cli": "^5.0.0",
    "eslint": "^9.30.1",
    "memfs": "^4.17.2",
    "rollup": "^4.42.0",
    "tsup": "^8.5.0",
    "type-fest": "^4.41.0",
    "typescript": "^5.8.3",
    "vitest": "^3.2.4",
    "@internal/types-builder": "workspace:*"
